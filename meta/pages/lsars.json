{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "Lsars"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "icon": "add_circle_outline",
                  "name": "button0",
                  "style": {
                    "marginBottom": "10px"
                  },
                  "text": "Add",
                  "type": "button",
                  "events": {
                    "click": [
                      {
                        "path": "add-lsar",
                        "type": "popup",
                        "then": [
                          {
                            "code": "await grid0.Reload();",
                            "type": "execute"
                          },
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "events": {
                    "click": [
                      {
                        "condition": "${event?.Value == \"csv\"}",
                        "fileName": "Lsars",
                        "name": "Telemetry Server.getLsars",
                        "parameters": [
                          {
                            "name": "$filter",
                            "value": "${(string.IsNullOrEmpty(grid0.Query.Filter)? \"true\" : grid0.Query.Filter)}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${grid0.Query.OrderBy}"
                          },
                          {
                            "name": "$expand",
                            "value": ""
                          },
                          {
                            "name": "$select",
                            "value": "id,sender,room,time,priority_tag,encoded_lat,encoded_lon,pnt_source,condition_state,vmc_txt,tac_sn,cntry_code,homing_dvc_stat,ret_lnk_stat,test_proto,vessel_id,beacon_type,createdAt,updatedAt"
                          }
                        ],
                        "type": "export"
                      },
                      {
                        "condition": "${event == null || event.Value == \"xlsx\"}",
                        "fileName": "Lsars",
                        "format": "xlsx",
                        "name": "Telemetry Server.getLsars",
                        "parameters": [
                          {
                            "name": "$filter",
                            "value": "${(string.IsNullOrEmpty(grid0.Query.Filter)? \"true\" : grid0.Query.Filter)}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${grid0.Query.OrderBy}"
                          },
                          {
                            "name": "$expand",
                            "value": ""
                          },
                          {
                            "name": "$select",
                            "value": "id,sender,room,time,priority_tag,encoded_lat,encoded_lon,pnt_source,condition_state,vmc_txt,tac_sn,cntry_code,homing_dvc_stat,ret_lnk_stat,test_proto,vessel_id,beacon_type,createdAt,updatedAt"
                          }
                        ],
                        "type": "export"
                      }
                    ]
                  },
                  "icon": "get_app",
                  "items": [
                    {
                      "icon": "",
                      "text": "Excel",
                      "value": "xlsx"
                    },
                    {
                      "icon": "",
                      "text": "CSV",
                      "value": "csv"
                    }
                  ],
                  "name": "splitbutton0",
                  "style": {
                    "marginLeft": "10px",
                    "marginBottom": "10px"
                  },
                  "text": "Export",
                  "type": "splitbutton"
                },
                {
                  "attributes": [
                    {
                      "name": "@oninput",
                      "value": "@(async(args) => {search = $\"{args.Value}\";await grid0.GoToPage(0);await grid0.Reload();})"
                    }
                  ],
                  "name": "textbox0",
                  "placeholder": "Search ...",
                  "style": {
                    "display": "block",
                    "marginBottom": "10px",
                    "width": "100%"
                  },
                  "type": "textbox",
                  "value": ""
                },
                {
                  "name": "grid0",
                  "allowFiltering": true,
                  "filterMode": "advanced",
                  "allowPaging": true,
                  "allowSorting": true,
                  "data": "${getLsarsResult}",
                  "events": {
                    "loadData": [
                      {
                        "name": "Telemetry Server.getLsars",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "event.top"
                          },
                          {
                            "name": "$skip",
                            "value": "event.skip"
                          },
                          {
                            "name": "$count",
                            "value": "event.top != null && event.skip != null"
                          },
                          {
                            "name": "$filter",
                            "value": [
                              {
                                "filters": [
                                  {
                                    "operation": "contains",
                                    "property": "time",
                                    "type": "string",
                                    "value": "\"${search}\""
                                  },
                                  {
                                    "operation": "contains",
                                    "property": "priority_tag",
                                    "type": "string",
                                    "value": "\"${search}\""
                                  },
                                  {
                                    "operation": "contains",
                                    "property": "encoded_lat",
                                    "type": "string",
                                    "value": "\"${search}\""
                                  },
                                  {
                                    "operation": "contains",
                                    "property": "encoded_lon",
                                    "type": "string",
                                    "value": "\"${search}\""
                                  },
                                  {
                                    "operation": "contains",
                                    "property": "pnt_source",
                                    "type": "string",
                                    "value": "\"${search}\""
                                  },
                                  {
                                    "operation": "contains",
                                    "property": "condition_state",
                                    "type": "string",
                                    "value": "\"${search}\""
                                  },
                                  {
                                    "operation": "contains",
                                    "property": "vmc_txt",
                                    "type": "string",
                                    "value": "\"${search}\""
                                  },
                                  {
                                    "operation": "contains",
                                    "property": "tac_sn",
                                    "type": "string",
                                    "value": "\"${search}\""
                                  },
                                  {
                                    "operation": "contains",
                                    "property": "cntry_code",
                                    "type": "string",
                                    "value": "\"${search}\""
                                  },
                                  {
                                    "operation": "contains",
                                    "property": "homing_dvc_stat",
                                    "type": "string",
                                    "value": "\"${search}\""
                                  },
                                  {
                                    "operation": "contains",
                                    "property": "ret_lnk_stat",
                                    "type": "string",
                                    "value": "\"${search}\""
                                  },
                                  {
                                    "operation": "contains",
                                    "property": "test_proto",
                                    "type": "string",
                                    "value": "\"${search}\""
                                  },
                                  {
                                    "operation": "contains",
                                    "property": "vessel_id",
                                    "type": "string",
                                    "value": "\"${search}\""
                                  },
                                  {
                                    "operation": "contains",
                                    "property": "beacon_type",
                                    "type": "string",
                                    "value": "\"${search}\""
                                  }
                                ],
                                "operator": "or"
                              },
                              {
                                "value": "${(string.IsNullOrEmpty(event.filter)? \"true\" : event.filter)}"
                              }
                            ],
                            "operator": "and"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          }
                        ],
                        "then": [
                          {
                            "name": "getLsarsResult",
                            "type": "set",
                            "value": "${result.Value.AsODataEnumerable()}",
                            "valueType": "IEnumerable<Hms.Models.TelemetryServer.Lsar>"
                          },
                          {
                            "name": "getLsarsCount",
                            "type": "set",
                            "value": "${result.Count}",
                            "valueType": "int"
                          }
                        ],
                        "type": "invoke",
                        "error": [
                          {
                            "detail": "Unable to load Lsars",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ]
                      }
                    ],
                    "rowDoubleClick": [
                      {
                        "parameters": [
                          {
                            "name": "id",
                            "value": "${event.Data.id}"
                          }
                        ],
                        "path": "edit-lsar",
                        "type": "popup",
                        "then": [
                          {
                            "code": "await grid0.Reload();",
                            "type": "execute"
                          },
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  },
                  "title": "",
                  "columns": [
                    {
                      "property": "id",
                      "title": "Id"
                    },
                    {
                      "property": "sender",
                      "title": "Sender"
                    },
                    {
                      "property": "room",
                      "title": "Room"
                    },
                    {
                      "property": "time",
                      "title": "Time"
                    },
                    {
                      "property": "priority_tag",
                      "title": "Priority Tag"
                    },
                    {
                      "property": "encoded_lat",
                      "title": "Encoded Lat"
                    },
                    {
                      "property": "encoded_lon",
                      "title": "Encoded Lon"
                    },
                    {
                      "property": "pnt_source",
                      "title": "Pnt Source"
                    },
                    {
                      "property": "condition_state",
                      "title": "Condition State"
                    },
                    {
                      "property": "vmc_txt",
                      "title": "Vmc Txt"
                    },
                    {
                      "property": "tac_sn",
                      "title": "Tac Sn"
                    },
                    {
                      "property": "cntry_code",
                      "title": "Cntry Code"
                    },
                    {
                      "property": "homing_dvc_stat",
                      "title": "Homing Dvc Stat"
                    },
                    {
                      "property": "ret_lnk_stat",
                      "title": "Ret Lnk Stat"
                    },
                    {
                      "property": "test_proto",
                      "title": "Test Proto"
                    },
                    {
                      "property": "vessel_id",
                      "title": "Vessel Id"
                    },
                    {
                      "property": "beacon_type",
                      "title": "Beacon Type"
                    },
                    {
                      "property": "createdAt",
                      "formatString": "{0:MM/dd/yyyy}",
                      "title": "Created At"
                    },
                    {
                      "property": "updatedAt",
                      "formatString": "{0:MM/dd/yyyy}",
                      "title": "Updated At"
                    },
                    {
                      "filterable": false,
                      "sortable": false,
                      "width": "70px",
                      "textAlign": "center",
                      "template": [
                        {
                          "attributes": [
                            {
                              "name": "@onclick:stopPropagation",
                              "value": true
                            }
                          ],
                          "buttonStyle": "danger",
                          "icon": "close",
                          "name": "gridDeleteButton",
                          "text": "",
                          "size": "sm",
                          "type": "button",
                          "events": {
                            "click": [
                              {
                                "condition": "await DialogService.Confirm(\"Are you sure you want to delete this record?\") == true",
                                "error": [
                                  {
                                    "detail": "Unable to delete Lsar",
                                    "severity": "error",
                                    "summary": "Error",
                                    "type": "notify"
                                  }
                                ],
                                "name": "Telemetry Server.deleteLsar",
                                "parameters": [
                                  {
                                    "name": "id",
                                    "value": "${data.id}"
                                  }
                                ],
                                "then": [
                                  {
                                    "code": "await grid0.Reload();",
                                    "condition": "result != null && result.StatusCode == System.Net.HttpStatusCode.NoContent",
                                    "type": "execute"
                                  },
                                  {
                                    "condition": "result != null && result.StatusCode != System.Net.HttpStatusCode.NoContent",
                                    "detail": "Unable to delete Lsar",
                                    "severity": "error",
                                    "summary": "Error",
                                    "type": "notify"
                                  }
                                ],
                                "type": "invoke"
                              }
                            ]
                          }
                        }
                      ]
                    }
                  ],
                  "type": "datagrid",
                  "count": "${getLsarsCount}",
                  "itemType": "Hms.Models.TelemetryServer.Lsar"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "condition": "string.IsNullOrEmpty(search)",
        "name": "search",
        "type": "set",
        "value": "\"\"",
        "valueType": "string"
      }
    ]
  },
  "name": "Lsars",
  "access": [
    "Authenticated"
  ],
  "layout": "Main"
}