  "events": {
    "load": [
      {
        "name": "Telemetry Server.getSimulationuiumByid",
        "parameters": [
          {
            "name": "id",
            "value": "${parameters.id}"
          }
        ],
        "then": [
          {
            "name": "simulationuium",
            "type": "set",
            "value": "${result}",
            "valueType": "Hms.Models.TelemetryServer.Simulationuium"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
