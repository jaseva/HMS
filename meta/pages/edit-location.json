{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Id",
                              "component": "id",
                              "name": "idLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int?",
                              "type": "numeric",
                              "name": "id",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${location.id}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "User",
                              "component": "user",
                              "name": "userLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "user",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${location.user}"
                            },
                            {
                              "type": "required-validator",
                              "name": "userRequiredValidator",
                              "component": "user",
                              "text": "user is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Room",
                              "component": "room",
                              "name": "roomLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "room",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${location.room}"
                            },
                            {
                              "type": "required-validator",
                              "name": "roomRequiredValidator",
                              "component": "room",
                              "text": "room is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Created At",
                              "component": "createdAt",
                              "name": "createdAtLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime",
                              "dateFormat": "MM/dd/yyyy",
                              "type": "datepicker",
                              "name": "createdAt",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${location.createdAt}"
                            },
                            {
                              "type": "required-validator",
                              "name": "createdAtRequiredValidator",
                              "component": "createdAt",
                              "text": "createdAt is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Updated At",
                              "component": "updatedAt",
                              "name": "updatedAtLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime",
                              "dateFormat": "MM/dd/yyyy",
                              "type": "datepicker",
                              "name": "updatedAt",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${location.updatedAt}"
                            },
                            {
                              "type": "required-validator",
                              "name": "updatedAtRequiredValidator",
                              "component": "updatedAt",
                              "text": "updatedAt is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update Location",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Telemetry Server.updateLocation",
                        "parameters": [
                          {
                            "name": "id",
                            "value": "${parameters.id}"
                          },
                          {
                            "name": "Location",
                            "value": "${location}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${location}",
                            "condition": ""
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Hms.Models.TelemetryServer.Location",
                  "data": "${location}",
                  "visible": "${location != null}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "Telemetry Server.getLocationByid",
        "parameters": [
          {
            "name": "id",
            "value": "${parameters.id}"
          }
        ],
        "then": [
          {
            "name": "location",
            "type": "set",
            "value": "${result}",
            "valueType": "Hms.Models.TelemetryServer.Location"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Location",
  "access": [
    "Authenticated"
  ],
  "layout": "Main"
}