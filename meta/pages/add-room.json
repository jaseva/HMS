{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Id",
                              "component": "id",
                              "name": "idLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int?",
                              "type": "numeric",
                              "name": "id",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${room.id}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Name",
                              "component": "name",
                              "name": "nameLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "textbox",
                              "name": "name",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${room.name}"
                            },
                            {
                              "type": "required-validator",
                              "name": "nameRequiredValidator",
                              "component": "name",
                              "text": "name is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Users",
                              "component": "users",
                              "name": "usersLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "users",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${room.users}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Created At",
                              "component": "createdAt",
                              "name": "createdAtLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime",
                              "dateFormat": "MM/dd/yyyy",
                              "type": "datepicker",
                              "name": "createdAt",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${room.createdAt}"
                            },
                            {
                              "type": "required-validator",
                              "name": "createdAtRequiredValidator",
                              "component": "createdAt",
                              "text": "createdAt is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Updated At",
                              "component": "updatedAt",
                              "name": "updatedAtLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime",
                              "dateFormat": "MM/dd/yyyy",
                              "type": "datepicker",
                              "name": "updatedAt",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${room.updatedAt}"
                            },
                            {
                              "type": "required-validator",
                              "name": "updatedAtRequiredValidator",
                              "component": "updatedAt",
                              "text": "updatedAt is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new Room!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Telemetry Server.createRoom",
                        "parameters": [
                          {
                            "name": "Room",
                            "value": "${room}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${room}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${room}",
                  "visible": "${room != null}",
                  "itemType": "Hms.Models.TelemetryServer.Room"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "room",
        "type": "set",
        "value": "new Hms.Models.TelemetryServer.Room(){}",
        "valueType": "Hms.Models.TelemetryServer.Room"
      }
    ]
  },
  "name": "Add Room",
  "access": [
    "Authenticated"
  ],
  "layout": "Main"
}